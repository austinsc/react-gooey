{
  "name": "react-gooey",
  "description": "Gooey",
  "authors": [
    "Stephen C. Austin <stephen.austin@pwnt.co>",
    "Emily Moore"
  ],
  "user": "survivejs",
  "version": "0.0.0",
  "scripts": {
    "start": "webpack-dev-server",
    "test": "karma start --",
    "test:tdd": "karma start --auto-watch --no-single-run --",
    "test:lint": "eslint . --ext .js --ext .jsx --ignore-path .gitignore --ignore-pattern dist --cache",
    "gh-pages": "webpack",
    "gh-pages:deploy": "gh-pages -d gh-pages",
    "gh-pages:stats": "webpack --profile --json > stats.json",
    "dist": "webpack",
    "dist:min": "webpack",
    "dist:modules": "rimraf ./dist-modules && babel ./src --out-dir ./dist-modules",
    "pretest": "npm run test:lint",
    "preversion": "npm run test && npm run dist && npm run dist:min && git commit --allow-empty -am \"Update dist\"",
    "prepublish": "npm run dist:modules",
    "postpublish": "npm run gh-pages && npm run gh-pages:deploy",
    "postinstall": "node lib/post_install.js"
  },
  "main": "dist-modules",
  "module": "src",
  "jsnext:main": "src",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.9",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.6",
    "catalog": "^2.4.4",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.10",
    "css-loader": "^0.23.1",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "gh-pages": "^0.11.0",
    "git-prepush-hook": "^1.0.1",
    "html-webpack-plugin": "^2.22.0",
    "html-webpack-template": "^5.0.0",
    "isparta-loader": "^2.0.0",
    "json-loader": "^0.5.4",
    "karma": "^1.1.2",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.7.0",
    "mocha": "^3.0.0",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "^2.1.9",
    "purecss": "^0.6.0",
    "raw-loader": "^0.5.1",
    "react": "^15.3.0",
    "react-addons-test-utils": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-github-corner": "^0.3.0",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "sync-exec": "^0.6.2",
    "system-bell-webpack-plugin": "^1.0.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.1"
  },
  "peerDependencies": {
    "react": ">= 0.11.2 < 16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/austinsc/react-gooey.git"
  },
  "homepage": "https://austinsc.github.io/react-gooey/",
  "bugs": {
    "url": "https://github.com/austinsc/react-gooey/issues"
  },
  "keywords": [
    "react",
    "reactjs",
    "components",
    "gooey",
    "gui",
    "forms",
    "controls"
  ],
  "license": "MIT",
  "pre-push": [
    "test",
    "test:lint"
  ]
}
