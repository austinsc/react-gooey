    "prop-types": "^15.5.10",
    "react-docgen": "^2.15.0",
    "react-jsonschema-form": "^0.48.2",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-style-decorator": "0.0.8",
    "react-syntax-highlighter": "^5.6.2"
  }
}
