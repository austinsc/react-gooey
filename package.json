    "webpack-dashboard": "^0.4.0",
    "webpack-dev-server": "^2.4.5",
    "webpack-merge": "^4.1.0"
  },
  "peerDependencies": {
    "react": ">= 0.11.2 < 16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/austinsc/react-gooey.git"
  },
  "homepage": "https://austinsc.github.io/react-gooey/",
  "bugs": {
    "url": "https://github.com/austinsc/react-gooey/issues"
  },
  "keywords": [
    "react",
    "reactjs",
    "components",
    "gooey",
    "gui",
    "forms",
    "controls"
  ],
  "license": "MIT",
